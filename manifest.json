{
    "manifest_version": 3,
    "name": "Net Blocker",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "storage"
    ]
}